{
  "name": "@netcracker/qubership-apihub-api-doc-viewer",
  "version": "2.1.1",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "default": "./dist/style.css"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "npm run build:dist",
    "build:dist": "tsc && vite build",
    "build:showcase": "storybook build -o dist-showcase --disable-telemetry",
    "dev": "vite",
    "development:link": "npm link && npm link @netcracker/qubership-apihub-api-data-model && npm link @netcracker/qubership-apihub-graphapi && npm link @netcracker/qubership-apihub-api-diff && npm link @netcracker/qubership-apihub-api-unifier && npm link @netcracker/qubership-apihub-json-crawl",
    "development:unlink": "npm unlink && npm unlink @netcracker/qubership-apihub-api-data-model && npm unlink @netcracker/qubership-apihub-graphapi && npm unlink @netcracker/qubership-apihub-api-diff && npm unlink @netcracker/qubership-apihub-api-unifier && npm unlink @netcracker/qubership-apihub-json-crawl",
    "generate-stories": "node bin/generate-graphql-compatibility-suite-stories.mjs",
    "generate-tests": "node bin/generate-graphql-compatibility-suite-tests.mjs",
    "postinstall": "npm run generate-stories & npm run generate-tests",
    "development:storybook": "storybook dev -p 9009",
    "development:local-server:static": "npm run build:showcase && ws -p 9009 -d dist-showcase --spa index.html --static.maxage 3600",
    "screenshot-test:ci": "start-server-and-test development:local-server:static http://localhost:9009 screenshot-test:run-test:ci",
    "screenshot-test:docker": "start-server-and-test development:local-server:static http://localhost:9009 screenshot-test:run-test:docker",
    "screenshot-test:run-test:ci": "jest --maxWorkers 4 -c .config/it/it-test.jest.config.cjs",
    "screenshot-test:run-test:docker": "jest --maxWorkers 4 -c .config/it/it-test-docker.jest.config.cjs",
    "screenshot-test:regenerate-snapshots": "start-server-and-test development:local-server:static http://localhost:9009 screenshot-test:regenerate-snapshots:run",
    "screenshot-test:regenerate-snapshots:run": "jest --maxWorkers 4 --updateSnapshot -c .config/it/it-test-docker.jest.config.cjs",
    "lint:check": "eslint -c .eslintrc.json . --ext .js,.cjs,.ts,.json --ignore-path .gitignore --max-warnings 0 --report-unused-disable-directives",
    "lint:fix": "npm run lint:check -- --fix",
    "preview": "vite preview",
    "update-lock-file": "update-lock-file @netcracker"
  },
  "eslintConfig": {
    "extends": ".eslintrc.json"
  },
  "resolutions": {
    "jackspeak": "2.1.1",
    "strip-ansi": "6.0.1"
  },
  "dependencies": {
    "@netcracker/qubership-apihub-api-data-model": "next",
    "@netcracker/qubership-apihub-api-diff": "next",
    "react-markdown": "8.0.7",
    "react-use": "^17.6.0",
    "remark-gfm": "3.0.1"
  },
  "devDependencies": {
    "@netcracker/qubership-apihub-api-unifier": "2.0.0",
    "@netcracker/qubership-apihub-compatibility-suites": "2.0.3",
    "@netcracker/qubership-apihub-graphapi": "1.0.8",
    "@netcracker/qubership-apihub-npm-gitflow": "3.1.0",
    "@netcracker/qubership-apihub-jest-chrome-in-docker-environment": "2.0.0",
    "@storybook/addon-essentials": "8.1.11",
    "@storybook/addon-interactions": "8.1.11",
    "@storybook/addon-links": "8.1.11",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/blocks": "8.1.11",
    "@storybook/react": "8.1.11",
    "@storybook/react-vite": "8.1.11",
    "@storybook/testing-library": "0.2.0",
    "@types/fontfaceobserver": "2.1.0",
    "@types/jest": "29.5.1",
    "@types/jest-environment-puppeteer": "5.0.3",
    "@types/jest-image-snapshot": "6.1.0",
    "@types/json-schema": "^7.0.12",
    "@types/react": "^18.0.0",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitejs/plugin-react": "4.0.3",
    "autoprefixer": "10.4.15",
    "docker-chromium": "1.4.2",
    "dpdm": "3.14.0",
    "eslint": "8.48.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "eslint-plugin-storybook": "0.6.13",
    "find-node-modules": "2.1.3",
    "fontfaceobserver": "^2.3.0",
    "graphql": "16.9.0",
    "jest": "29.5.0",
    "jest-environment-puppeteer": "7.0.1",
    "jest-image-snapshot": "6.1.0",
    "jest-image-snapshot-diff-reporter": "1.0.0",
    "jest-puppeteer": "7.0.1",
    "local-web-server": "5.3.0",
    "postcss": "8.4.29",
    "puppeteer": "21.11.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-remove-scroll": "^2.6.0",
    "start-server-and-test": "2.0.0",
    "storybook": "8.1.11",
    "tailwindcss": "3.3.3",
    "ts-jest": "29.1.0",
    "typescript": "5.0.2",
    "vite": "4.4.5",
    "vite-plugin-dts": "3.5.3"
  }
}